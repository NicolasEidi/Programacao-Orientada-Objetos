package exercicio;

public abstract class Cliente extends Endereco {
	private String nome;
	private String dataNascimento;
	private int telefone;

	public Cliente(String logradouro, int numResidencia, String bairro, int cep, String cidade, String uf, String nome,
			String dataNascimento, int telefone) {
		super(logradouro, numResidencia, bairro, cep, cidade, uf);
		this.nome = nome;
		this.dataNascimento = dataNascimento;
		this.telefone = telefone;
	}

}
