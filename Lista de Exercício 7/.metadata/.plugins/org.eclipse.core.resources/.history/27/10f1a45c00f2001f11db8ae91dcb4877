package exercicio;

public class Socio {
	private Dependente dependente;
	
	public Socio(Dependente dependente) {
		this.setDependente(null);
	}

	public Dependente getDependente() {
		return dependente;
	}

	public void setDependente(Dependente dependente) {
		this.dependente = dependente;
	}

	public void adicionarDependente(String logradouro, int numResidencia, String bairro, int cep, String cidade, String uf,
			String nome, String dataNascimento, int telefone) {
		this.setDependente(new Dependente(logradouro, numResidencia, bairro, cep, cidade, uf, nome, dataNascimento, telefone));
	}
}
